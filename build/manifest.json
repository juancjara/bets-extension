{
  "manifest_version": 2,
  "name": "betsson notifier",
  "description": "betsson",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "https://ajax.googleapis.com/",
    "https://sbfacade.bpsgameserver.com/",
    "storage",
    "alarms",
    "notifications"
  ],
  "background": {
    "scripts": ["js/background-bundle.js"]
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
